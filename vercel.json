{
  "version": 2,
  "build": {
    "env": {
      "CI": "1"
    },
    "commands": {
      "build": "npm run build"
    },
    "postbuild": "npx cypress install"
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}